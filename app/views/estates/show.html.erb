<h1>show</h1>
<table>
  <tr>
    <th>物件名</th>
    <th>賃料</th>
    <th>住所</th>
    <th>築年数</th>
    <th>備考</th>
  </tr>
  <tr>
    <td><%= @estate.property_name %></td>
    <td><%= @estate.rent %></td>
    <td><%= @estate.address %></td>
    <td><%= @estate.property_age %></td>
    <td><%= @estate.content %></td>
    <td>詳細</td>
    <td>編集</td>
    <td>削除</td>
  </tr>
</table>

<% @station.each do |station| %>
<h2>最寄駅</h2>
<table>
  <tr>
    <th>路線名</th>
    <th>駅名</th>
    <th>徒歩分数</th>
  </tr>
  <tr>
    <td><%= station.route_name %></td>
    <td><%= station.station_name %></td>
    <td>徒歩<%= station.walking_minutes %>分</td>
  </tr>
</table>
<% end %>